{
    "version": 2,
    "builds": [
      {
        "src": "quote/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "quote/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ],
    "functions": {
      "api/**/*.py": {
        "runtime": "python3.9"
      }
    }
  }